
package forms;

public class Calculator {

	private double	x;
	private double	y;
	private String	operator;
	private double	result;


	public double getX() {
		return this.x;
	}

	public double getY() {
		return this.y;
	}
	
	@Pattern(regexp="^[\\+\\-\\*\\/]$")
	public String getOperator(){
		return this.operator;
	}
}
